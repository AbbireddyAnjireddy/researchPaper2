ay.\n","\"\"\"\n","\n","    # ‚úÖ JSON extractor\n","    def extract_emails(text, batch_num, attempt=\"Initial\"):\n","        cleaned = text.strip().replace(\"‚Äò\", \"'\").replace(\"‚Äô\", \"'\").replace(\"‚Äú\", '\"').replace(\"‚Äù\", '\"')\n","        cleaned = re.sub(r\"^```json|```$\", \"\", cleaned.strip(), flags=re.MULTILINE)\n","        match = re.search(r\"\\[\\s*{.*?}\\s*\\]\", cleaned, re.DOTALL)\n","        if match:\n","            try:\n","                return json.loads(match.group(0))\n","            except Exception as e:\n","                print(f\"‚ùå {attempt} JSON decode failed in batch {batch_num}: {e}\")\n","        else:\n","            print(f\"‚ö†Ô∏è {attempt}: No JSON block found in batch {batch_num}\")\n","        return []\n","\n","    # üîÅ Generate 10 batches of 10 emails = 100 total\n","    for i in range(10):\n","        print(f\"\\nüí∞ Generating batch {i+1}/10 for domain: {domain}\")\n","        prompt = prompt_template.format(num=10)\n","\n","        message = client.messages.create(\n","            model=\"claude-3-haiku-20240307\",\n","            max_tokens=2000,\n","            messages=[{\"role\": \"user\", \"content\": prompt}]\n","        )\n","        raw_text = message.content[0].text\n","        batch_data = extract_emails(raw_text, i+1)\n","\n","        if not batch_data and ENABLE_RETRY:\n","            print(f\"üîÅ Retrying batch {i+1}...\")\n","            retry_msg = client.messages.create(\n","                model=\"claude-3-haiku-20240307\",\n","                max_tokens=2000,\n","                messages=[{\"role\": \"user\", \"content\": prompt}]\n","            )\n","            retry_text = retry_msg.content[0].text\n","            batch_data = extract_emails(retry_text, i+1, attempt=\"Retry\")\n","\n","        if batch_data:\n","            all_entries.extend(batch_data)\n","        else:\n","            print(f\"‚ùå Batch {i+1} failed{' after retry' if ENABLE_RETRY else ''}.\")\n","\n","    # üîÑ Fill missing entries if needed\n","    while len(all_entries) \u003c 100:\n","        missing = 100 - len(all_entries)\n","        print(f\"üîÑ Only {len(all_entries)} emails collected. Generating {missing} more...\")\n","        prompt = prompt_template.format(num=missing)\n","\n","        topup_msg = client.messages.create(\n","            model=\"claude-3-haiku-20240307\",\n","            max_tokens=2000,\n","            messages=[{\"role\": \"user\", \"content\": prompt}]\n","        )\n","        topup_text = topup_msg.content[0].text\n","        topup_data = extract_emails(topup_text, \"top-up\", attempt=\"Top-up\")\n","        all_entries.extend(topup_data[:missing])\n","\n","    # ‚úÖ Save to Excel\n","    if all_entries:\n","        df = pd.DataFrame(all_entries)\n","        pd.set_option('display.max_rows', None)\n","        pd.set_option('display.max_columns', None)\n","\n","        df.to_excel(file_output, index=False)\n","        print(f\"\\n‚úÖ Saved {len(all_entries)} {domain} emails to Excel: {file_output}\")\n","        display(df)\n","        files.download(file_output)\n","    else:\n","        print(\"‚ùå No valid entries were generated.\")\n"]},{"cell_type":"markdown","metadata":{"id":"jMEFhrMnVCDo"},"source":["Hospital"]},{"cell_type":"code","execution_count":null,"metadata":{"colab":{"background_save":true},"id":"aVfKzxIqUcbJ"},"outputs":[],"source":["import os\n","import json\n","import re\n","import pandas as pd\n","from IPython.display import display\n","from google.colab import files\n","\n","# ‚úÖ Configuration\n","domain = \"retail\"\n","field_proof = f\"{domain}_proof\"\n","file_output = f\"/content/{domain}_email_dataset_100.xlsx\"\n","ENABLE_RETRY = True  # Set False if you want to skip retries\n","\n","# ‚úÖ Skip generation if file already exists\n","if os.path.exists(file_output):\n","    print(f\"‚úÖ [SKIPPED] {domain} dataset already exists: {file_output}\")\n","    files.download(file_output)\n","\n","else:\n","    all_entries = []\n","\n","    # ‚úÖ Prompt template for generating emails\n","    prompt_template = f\"\"\"\n","You are a strict JSON-generating assistant working in the {domain} cybersecurity domain.\n","\n","Instructions:\n","- Generate exactly {{num}} synthetic email samples related to the {domain} domain.\n","- 50% must be labeled \"Phishing\", 50% \"Legitimate\"\n","- Each must include:\n","  - sender, receiver, date (\"YYYY-MM-DD HH:MM:SS\"), subject, body\n","  - label (\"Phishing\" or \"Legitimate\")\n","  - label_verification (reason for label)\n","  - {field_proof} (keyword or proof showing it's {domain}-related)\n","- Randomize order\n","- ‚ö†Ô∏è Output ONLY a valid raw JSON array of {{num}} email objects\n","- DO NOT include explanations, notes, code blocks, or formatting ‚Äî just the JSON array.\n","\"\"\"\n","\n","    # ‚úÖ JSON extractor with cleanup\n","    def extract_emails(text, batch_num, attempt=\"Initial\"):\n","        cleaned = text.strip().replace(\"‚Äò\", \"'\").replace(\"‚Äô\", \"'\").replace(\"‚Äú\", '\"').replace(\"‚Äù\", '\"')\n","        cleaned = re.sub(r\"^```json|```$\", \"\", cleaned.strip(), flags=re.MULTILINE)\n","        match = re.search(r\"\\[\\s*{.*?}\\s*\\]\", cleaned, re.DOTALL)\n","        if match:\n","            try:\n","                return json.loads(match.group(0))\n","            except Exception as e:\n","                print(f\"‚ùå {attempt} JSON decode failed in batch {batch_num}: {e}\")\n","        else:\n","            print(f\"‚ö†Ô∏è {attempt}: No JSON block found in batch {batch_num}\")\n","        return []\n","\n","    # üîÅ Generate 10 batches of 10 emails = 100 total\n","    for i in range(10):\n","        print(f\"\\nüõçÔ∏è Generating batch {i+1}/10 for domain: {domain}\")\n","        prompt = prompt_template.format(num=10)\n","\n","        message = client.messages.create(\n","            model=\"claude-3-haiku-20240307\",\n","            max_tokens=2000,\n","            messages=[{\"role\": \"user\", \"content\": prompt}]\n","        )\n","        raw_text = message.content[0].text\n","        batch_data = extract_emails(raw_text, i+1)\n","\n","        if not batch_data and ENABLE_RETRY:\n","            print(f\"üîÅ Retrying batch {i+1}...\")\n","            retry_msg = client.messages.create(\n","                model=\"claude-3-haiku-20240307\",\n","                max_tokens=2000,\n","                messages=[{\"role\": \"user\", \"content\": prompt}]\n","            )\n","            retry_text = retry_msg.content[0].text\n","            batch_data = extract_emails(retry_text, i+1, attempt=\"Retry\")\n","\n","        if batch_data:\n","            all_entries.extend(batch_data)\n","        else:\n","            print(f\"‚ùå Batch {i+1} failed{' after retry' if ENABLE_RETRY else ''}.\")\n","\n","    # üîÑ Fill if total is \u003c 100\n","    while len(all_entries) \u003c 100:\n","        missing = 100 - len(all_entries)\n","        print(f\"üîÑ Only {len(all_entries)} emails collected. Generating {missing} more...\")\n","        prompt = prompt_template.format(num=missing)\n","\n","        topup_msg = client.messages.create(\n","            model=\"claude-3-haiku-20240307\",\n","            max_tokens=2000,\n","            messages=[{\"role\": \"user\", \"content\": prompt}]\n","        )\n","        topup_text = topup_msg.content[0].text\n","        topup_data = extract_emails(topup_text, \"top-up\", attempt=\"Top-up\")\n","        all_entries.extend(topup_data[:missing])\n","\n","    # ‚úÖ Save and show the results\n","    if all_entries:\n","        df = pd.DataFrame(all_entries)\n","        pd.set_option('display.max_rows', None)\n","        pd.set_option('display.max_columns', None)\n","\n","        df.to_excel(file_output, index=False)\n","        print(f\"\\n‚úÖ Saved {len(all_entries)} {domain} emails to Excel: {file_output}\")\n","        display(df)\n","        files.download(file_output)\n","    else:\n","        print(\"‚ùå No valid entries were generate